{
  "expo": {
    "name": "azmita-activator",
    "slug": "azmita-activator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/azmita_logo_red.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/azmita_logo_red.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.azmita.activator",
      "adaptiveIcon": {
        "foregroundImage": "./assets/azmita_logo_red.png",
        "backgroundColor": "#1A0505"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.NFC"
      ],
      "navigationBar": {
        "barStyle": "light-content",
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-nfc-manager",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "44c16abe-9495-4135-aebc-5d97c16c6095"
      }
    },
    "owner": "exauherrera"
  }
}